extends layout

block content
    div(class='users-div')
        h2 Users Page
        table
            tr
                th ID
                th First Name 
                th Last Name 
                th Email
                th Age
            each user in users
                tr.tableRow
                    td #{user.user_id}
                    td #{user.first_name}
                    td #{user.last_name}
                    td #{user.email_address}
                    td #{user.age}
        a(class='edit-btn', href='/editUsers') Edit